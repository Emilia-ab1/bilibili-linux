{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "hook.js"
      ],
      "css": [
        "hook/common.css"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "hook/player.js",
        "hook/index.js",
        "hook/common.css",
        "hook/RoamingPage.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}